        <icon density="xxxhdpi" platform="android" src="resources/icons/android/icon-192-xxxhdpi.png" />
        <splash density="land-ldpi" platform="android" src="resources/screens/android/screen-ldpi-landscape.png" />
        <splash density="land-mdpi" platform="android" src="resources/screens/android/screen-mdpi-landscape.png" />
        <splash density="land-hdpi" platform="android" src="resources/screens/android/screen-hdpi-landscape.png" />
