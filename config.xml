        <splash density="port-ldpi" platform="android" src="resources/screens/android/screen-ldpi-portrait.png" />
        <splash density="port-mdpi" platform="android" src="resources/screens/android/screen-mdpi-portrait.png" />
        <splash density="port-hdpi" platform="android" src="resources/screens/android/screen-hdpi-portrait.png" />
        <splash density="port-xhdpi" platform="android" src="resources/screens/android/screen-xhdpi-portrait.png" />
        <splash density="port-xxhdpi" platform="android" src="resources/screens/android/screen-xxhdpi-portrait.png" />
        <splash density="port-xxxhdpi" platform="android" src="resources/screens/android/screen-xxxhdpi-portrait.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="ios-statusbarstyle" value="black-opaque" />
        <icon height="1024" src="resources/ios/icon/icon-1024.png" width="1024" />
